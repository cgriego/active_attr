source "https://rubygems.org"

gemspec :development_group => :test, :path => ".."

git "https://github.com/rails/rails.git", :branch => "main" do
  gem "activemodel"
  gem "activesupport"
  gem "actionpack"
end

gem "activemodel-serializers-xml", :group => :test, :git => "https://github.com/rails/activemodel-serializers-xml.git"
gem "rexml", :group => :test
gem "protected_attributes_continued", :group => :test, :git => "https://github.com/westonganger/protected_attributes_continued"
# 3.0 got released, but rails 7.1 still lock it out to 2.x
gem "rack", :group => :test, :git => "https://github.com/rack/rack.git", :branch => "2-2-stable"
